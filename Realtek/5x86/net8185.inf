HKR,,DefaultKeyID,,"0"
HKR,,DefaultKey0,,""
HKR,,DefaultKey1,,""
HKR,,DefaultKey2,,""
HKR,,DefaultKey3,,""

;;****************************************************************************
;; NT 5.0 parameters
;;****************************************************************************
[RTL8185.reg]
HKR, Ndi\Interfaces,            UpperRange, 0, "ndis5,mdcwifi"
;;HKR, Ndi\Interfaces,            UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces,            LowerRange, 0, "ethernet"
HKR, Ndi,                       Service,    0, "RTL8185"

[rtl8185.Service]
DisplayName     = %RTL8185.Service.DispName%
ServiceType     = 1                             ; %SERVICE_KERNEL_DRIVER%
StartType       = 3                             ; %SERVICE_DEMAND_START%
ErrorControl    = 1                             ; %SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\rtl8185.sys
LoadOrderGroup  = NDIS

[RTL8185.EventLog]
Addreg = RTL8185.AddEventLog.reg

[RTL8185.AddEventLog.reg]
HKR,    ,   EventMessageFile,   0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR,    ,   TypesSupported,     0x00010001, 7

;;****************************************************************************
;; Destination Directory
;;****************************************************************************
[DestinationDirs]
RTL8185.CopyFiles           = 12
RTL8185.CopyFiles98         = 11
DefaultDestDir              = 11        ; LDID_SYS

;;****************************************************************************
;; Source Files
;;****************************************************************************
[SourceDisksNames]
1=%DISKNAME%,,,

[SourceDisksFiles]
rtl8185.sys  = 1
;netr8185.inf  = 1
;netr8185.cat  = 1
;netr8185n.cat  = 1

