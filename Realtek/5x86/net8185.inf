[version]
Signature      = "$Chicago$"
Compatible     = 1
Class          = Net
ClassGUID      = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider       = %Realtek%                                 ;;
CatalogFile.NT    = net8185.cat                               ;; for WHQL certified
;CatalogFile.NT = net8185n.cat
DriverVer      = 03/08/2010,5.1115.0308.2010

[Manufacturer]
%Realtek%   = Realtek
%Surecom%   = Surecom

[ControlFlags]
ExcludeFromSelect     = PCI\VEN_10EC&DEV_8185&REV_20
ExcludeFromSelect     = PCI\VEN_10EC&DEV_8185&SUBSYS_818510EC&REV_20  
ExcludeFromSelect     = PCI\VEN_10EC&DEV_8185&SUBSYS_616510E8                      
ExcludeFromSelect     = PCI\VEN_10EC&DEV_8185&SUBSYS_13F210BD&REV_20  
ExcludeFromSelect     = PCI\VEN_10EC&DEV_8185&SUBSYS_14F210BD&REV_20            

[Realtek]
%RTL8185.DeviceDesc%  			= RTL8185.ndi, PCI\VEN_10EC&DEV_8185&REV_20
%RTL8185.DeviceDesc%  			= RTL8185.ndi, PCI\VEN_10EC&DEV_8185&SUBSYS_818510EC&REV_20
%RTL8185.DeviceDesc%  			= RTL8185.ndi, PCI\VEN_10EC&DEV_8185&SUBSYS_616518E8 

[Surecom]
%RTL8185.DeviceDesc_SURECOME_EP9321%  	= RTL8185.ndi, PCI\VEN_10EC&DEV_8185&SUBSYS_13F210BD&REV_20
%RTL8185.DeviceDesc_SURECOME_EP9428%  	= RTL8185.ndi, PCI\VEN_10EC&DEV_8185&SUBSYS_14F210BD&REV_20


;;****************************************************************************
;; Windows 9x
;;****************************************************************************
[RTL8185.ndi]
DriverVer       = 03/08/2010,1115
AddReg          = rtl8185.id.reg, RTL8185.common.reg, RTL8185.win.reg
CopyFiles       = RTL8185.CopyFiles98

;;****************************************************************************
;; NT 5.0
;;****************************************************************************
[RTL8185.ndi.NT]
AddReg          = RTL8185.common.reg, RTL8185.reg
Characteristics = 0x84
BusType         = 5
CopyFiles       = RTL8185.CopyFiles

[RTL8185.ndi.NT.Services]
AddService = rtl8185, 2, rtl8185.Service, RTL8185.EventLog

[RTL8185.CopyFiles]
rtl8185.sys,,,2

[RTL8185.CopyFiles98]
rtl8185.sys,,,2

;;----------------------------------------------------------------------------
;; RTL8185 Parameters
;;----------------------------------------------------------------------------
;;***************************************************************************
;; Windows 9x parameters
;;***************************************************************************
[rtl8185.id.reg]
HKR, Ndi,       DeviceID,       0,      "PCI\VEN_10EC&DEV_8185"

HKR, Ndi\Install,                       ,       0,      rtsnt.install
HKR, NDI\Params\NetworkAddress,         flag,   1,      30, 00, 00, 00

[RTL8185.win.reg]
;; key,subkey,valuname,type,value
HKR, ,  DriverDesc,     0,      "Realtek RTL8185 54M Wireless LAN Network Adapter"
HKR, ,  DevLoader,      0,      *ndis
HKR, ,  DeviceVxDs,     0,      rtl8185.sys
HKR, ,  EnumPropPages,  0,      netdi.dll,      EnumPropPages

;;params
HKR, ,  MediaType,      0,      1
HKR, ,  AdapterType,    0,      5
HKR, ,  BusType,        0,      5

;; NDIS Info
HKR, NDIS,              MajorNdisVersion,       1,      03
HKR, NDIS,              MinorNdisVersion,       1,      0a
HKR, NDIS,              LogDriverName,          0,      "RTL8185"

;; Interfaces
HKR, Ndi\Interfaces,    DefLower,               ,       "ethernet"
HKR, Ndi\Interfaces,    LowerRange,             ,       "ethernet"
HKR, Ndi\Interfaces,    DefUpper,               ,       "ndis3"
HKR, Ndi\Interfaces,    UpperRange,             ,       "ndis3"

;; Install sections
HKR, Ndi\Install,       ndis3,                  0,      "rtl185.ndis5"

