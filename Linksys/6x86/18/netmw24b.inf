[Version] 
Signature = "$Windows NT$" 
Compatible  = 1
Class=Net
ClassGUID={4D36E972-E325-11CE-BFC1-08002BE10318} 
Provider=%MRVL% 
;CatalogFile=MRVW24B.cat
DriverVer=12/08/2006, 1.0.0.1

;===========================================================================
; Source Media Information Sections
;===========================================================================
[SourceDisksNames]
1 = "Linksys installation disk 1",,,

[SourceDisksFiles]
; On Marvell installation disk 1 
MRVW24B.sys = 1

[DestinationDirs]
W8360USB.x32.CopyFiles  = 12 

DefaultDestDirs    = 11

[Manufacturer] 
%MRVL%=Marvell,NTx86

[Marvell.NTx86]
%MRVL8360USB.DeviceDesc% = W8360USBx32.ndi,USB\VID_13b1&PID_0029
;===========================================================================
; ControlFlags section
;===========================================================================
[ControlFlags]
ExcludeFromSelect = *


;***************************************************************************
;**         
;**          Windows DDInstall section
;**          
;***************************************************************************
;***************************************************************************
[W8360USBx32.ndi]
; "Characteristics" is (NCF_PHYSICAL | NCF_HAS_UI)
Characteristics = 0x84 
; "BusType" is PCI ,CardBus or USB
BusType         = 15 ;Is USB
AddReg          = W8360USB.reg, W8360USBx32.reg
CopyFiles       = W8360USB.x32.CopyFiles
*IfType 	= 71 ; If_TYPE_IEEE80211
*MediaType      = 16    ; NdisMediumNative802_11
*PhysicalMediaType = 9  ; NdisPhysicalMediumNative802_11

[W8360USBx32.ndi.Services]
AddService = MRV6X32U, 2, W8360USBx32.Service, CommonXP.EventLog

;===========================================================================
; W8335a PCI WinXP registers
;===========================================================================
[W8360USBx32.reg]
HKR, Ndi,            Service,    0, "MRV6X32U"
HKR, Ndi\Interfaces, UpperRange, 0, "ndis5"
HKR, Ndi\Interfaces, LowerRange, 0, "ethernet,wlan"


[W8360USB.x32.CopyFiles]
MRVW24B.sys,,,2 

[W8360USBx32.Service]
DisplayName    = %W8360USBx32.Service.DispName%
ServiceType    = 1 ;%SERVICE_KERNEL_DRIVER%
StartType      = 3 ;%SERVICE_DEMAND_START%
ErrorControl   = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary  = %12%\MRVW24B.sys
LoadOrderGroup = NDIS


[CommonXP.EventLog]
AddReg = CommonXP.AddEventLog.reg

[CommonXP.AddEventLog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7


;===========================================================================
; W8335a PCI common, MIB can be defined here
