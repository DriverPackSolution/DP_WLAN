[W8360USB.reg]
HKR, Ndi\params\AdhocGMode,ParamDesc,,"%AdhocMode%"
HKR, Ndi\params\AdhocGMode,default,,"1"
HKR, Ndi\params\AdhocGMode,type,,enum
HKR, Ndi\params\AdhocGMode\enum,"0",,"%AdhocGMode%"
HKR, Ndi\params\AdhocGMode\enum,"1",,"%AdhocBMode%" 

