HKR, NDI\params\DesiredSSID,                LimitText,      0, "32"
HKR, NDI\params\DesiredSSID,                optional,       0, "1"
;

;RTSThreshold
HKR, ,                                      RTSThreshold,    0, "2347"
HKR, Ndi\params\RTSThreshold,               ParamDesc,       0, %RTSThreshold%
HKR, Ndi\params\RTSThreshold,               default,         0, "2347"
