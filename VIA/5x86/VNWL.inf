[DestinationDirs]
DefaultDestDir          = 11
WET.CopyFiles.Win       = 11
WET.CopyFiles.NT        = 12
WET.SW.CopyFiles.NTx86  = 11
WET.SW.CopyFiles        = 11

[SourceDisksFiles]
VNWL.inf                = 1
;VNWL5A.sys              = 1
VNWL5B.sys              = 1
vuins32.dll             = 1
;vuins16.dll             = 1

[SourceDisksNames]
1 = %DiskName%,,,

