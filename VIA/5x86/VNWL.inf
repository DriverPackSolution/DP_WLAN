HKR, Ndi\params\PowerSaveMode\enum,         1,              0, %FastPSP%
HKR, Ndi\params\PowerSaveMode\enum,         2,              0, %MaxPSP%

[WET.reg.win]

HKR, NDIS,                                  MajorNdisVersion,1,03
HKR, NDIS,                                  MinorNdisVersion,1,0A
HKR, ,                                      DevLoader,      0, "*ndis"
HKR, ,                                      DeviceVxDs,     0, "VNWL5A.sys"
HKR, NDIS,                                  LogDriverName,  0, "VNWLNDIS"

HKR, Ndi\Interfaces,                        DefLower,       0, "ethernet"
HKR, Ndi\Interfaces,                        LowerRange,     0, "ethernet"
HKR, Ndi\Interfaces,                        DefUpper,       0, "ndis3"
HKR, Ndi\Interfaces,                        UpperRange,     0, "ndis3"

;-------------------------------------------------------------------------------
; 16-bit UnInstaller
[WET.SW.win]
HKLM, Software\Microsoft\Windows\CurrentVersion\Uninstall\VN_VUIns_PCI_VNT, DisplayName, 0, %VT3253.DeviceDesc%
HKLM, Software\Microsoft\Windows\CurrentVersion\Uninstall\VN_VUIns_PCI_VNT, UninstallString, 0, "Rundll.exe vuins16.dll,vuins16Ex $PCI $VNT"
HKLM, Software\VN_VUIns\PCI\VNT\DeviceIdList, 3253, 0, "0"

[WET.reg.NT]
; Windows 2000/XP
HKR, Ndi\Interfaces,                        LowerRange,     0, "ethernet"
HKR, Ndi\Interfaces,                        UpperRange,     0, "ndis5"
HKR, Ndi,                                   Service,        0, "VNWLNDIS"
; ScatterGather DMA Capability
HKR, ,                                      ScatterGather,   0, "1"

;-------------------------------------------------------------------------------
; 32-bit UnInstaller
[WET.SW.NT]
HKLM, Software\Microsoft\Windows\CurrentVersion\Uninstall\VN_VUIns_PCI_VNT, DisplayName, 0, %VT3253.DeviceDesc%
HKLM, Software\Microsoft\Windows\CurrentVersion\Uninstall\VN_VUIns_PCI_VNT, UninstallString, 0, "Rundll32.exe vuins32.dll,vuins32Ex $PCI $VNT"
HKLM, Software\VN_VUIns\PCI\VNT\DeviceIdList, 3253, 0, "0"

;-------------------------------------------------------------------------------
; Installation for NT
;

[WET.CopyFiles.NT]
VNWL5B.sys,,,2
[WET.SW.CopyFiles.NTx86]
vuins32.dll,,,2

[WET.Service]
DisplayName     = %WET.Service.DispName%
ServiceType     = 1 ;%SERVICE_KERNEL_DRIVER%
StartType       = 3 ;%SERVICE_DEMAND_START%
ErrorControl    = 1 ;%SERVICE_ERROR_NORMAL%
ServiceBinary   = %12%\VNWL5B.sys
LoadOrderGroup  = NDIS

[WET.EventLog]
AddReg = WET.AddEventLog.reg

[WET.AddEventLog.reg]
HKR, , EventMessageFile, 0x00020000, "%%SystemRoot%%\System32\netevent.dll"
HKR, , TypesSupported,   0x00010001, 7


;-------------------------------------------------------------------------------
; CopyFiles sections for Windows 98SE/ME
;

[WET.CopyFiles.Win]
VNWL5A.sys,,,2

[WET.SW.CopyFiles]
vuins16.dll,,,2

