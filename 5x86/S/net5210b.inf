[Version]
Signature   = "$CHICAGO$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %SMC%
Compatible  = 1
DriverVer   = 11/26/2002,1.4.0.41
Catalogfile = net5210b.cat

[Manufacturer]
%SMC%     = SMC

[ControlFlags]
ExcludeFromSelect = *


[SMC]
; DisplayName           Section       DeviceID
; -----------           -------       --------
%ATHER.DeviceDesc%  = ATHER_DEV_1107.ndi,    PCI\VEN_168C&DEV_1107
%ATHER.DeviceDesc%  = ATHER_DEV_0005.ndi,    PCI\VEN_168C&DEV_0005
