; Windows 9X specific entries
[ATHER_DEV_1107.ndi]
AddReg          = ATHER_DEV_1107.id.reg, 5210.reg, ATHER.win.reg
CopyFiles       = ATHER.win.CopyFiles, ATHER.cpl.CopyFiles, DEFAULTDESTDIRS
DelFiles        = ATHER.DelIniFiles

[ATHER_DEV_0005.ndi]
