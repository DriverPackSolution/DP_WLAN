;*******************************************************
;AT76C503A based USB 11M Network Adapter 
; INF file for the NDIS driver
;
; Supported OS: Windows 98, Windows Me and Windows 2000
; SMC Version : V1.50
;*******************************************************



[Version]
Signature  = "$Windows NT$"
Class      = Net
ClassGUID  = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider   = %SMC%
;CatalogFile = netvusba.cat
DriverVer  = 01/02/2002,1.2.2.66

[Manufacturer]
%SMC%   = SMC

[ControlFlags]
ExcludeFromSelect = *


[SMC]
%USBFVNETA.DeviceDesc% = USBFVNETA.Ndi,USB\VID_0D5C&PID_A001

;*******************************************************************************
; Win 2000 main sections
;*******************************************************************************


[USBFVNETA.Ndi.NT]
Characteristics = 0x84  ; NCF_HAS_UI | NCF_PHYSICAL
BusType         = 15    ; USB
AddReg          = USBFVNETA.NT.Reg, USBFVNETA.Params
CopyFiles       = CopyFile.NT.Sys
DriverVer  = 01/02/2002,1.2.2.66

[USBFVNETA.Ndi.NT.Services]
AddService = USBFVNETA, 2, USBFVNETA.Service, Common.EventLog



;*******************************************************************************
; Win98 main sections


[USBFVNETA.Ndi]
AddReg     = USBFVNETA.Reg, USBFVNETA.Params

CopyFiles  = CopyFile.Sys
DriverVer  = 01/02/2002,1.2.2.66


;*******************************************************************************
; Windows 98 section
;*******************************************************************************

[USBFVNETA.Reg]
HKR,Ndi,DeviceID,,USB\VID_0D5C&PID_A001
HKR,Ndi,CardType,,"PNP"


HKR,,DevLoader,0,"*ndis"
HKR,,DeviceVxDs,0,"vnetusba.sys"

; NDIS Info
HKR,NDIS,MajorNdisVersion,1,05
HKR,NDIS,MinorNdisVersion,1,00
HKR,NDIS,LogDriverName,,VNETUSBA

; Interfaces
HKR,Ndi\Interfaces,DefUpper,,"ndis3"
HKR,Ndi\Interfaces,DefLower,,"ethernet"
HKR,Ndi\Interfaces,UpperRange,,"ndis3"
HKR,Ndi\Interfaces,LowerRange,,"ethernet"

HKR,Ndi,HelpText,,%Help_Text%


