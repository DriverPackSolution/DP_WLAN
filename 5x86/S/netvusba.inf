[Strings]
SMC = "SMC"
USBFVNETA.DeviceDesc    = "SMC2662W EZ Connect Wireless USB Adapter"
USBFVNETA.DisplayName   = "SMC2662W EZ Connect Wireless USB Adapter"
DriverDisketteName      = "SMC2662W EZ Connect Wireless USB Driver Disk"
Help_Text = "USB device lets you set up easily and operate a Wireless LAN compliant with 802.11 Standard. It operates in the license free 2.4 GHz ISM band and achieves a peak data rate of 11 Mbps."
