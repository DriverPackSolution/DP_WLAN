Class               = Net
ClassGUID           = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider            = %SiSMfg%
CatalogFile	    = sis163.cat
DriverVer           = 02/24/2005,5.1.1039.1030

[Manufacturer]
