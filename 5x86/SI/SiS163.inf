[ControlFlags]
;;ExcludeFromSelect   =PCI\VEN_1039&DEV_0163  
ExcludeFromSelect   =PCI\VEN_1039&DEV_0163&SUBSYS_01631039 
              
;----------------------------------------------------------------------------
; Device section
;
[SiS]
%sis163.DeviceDesc%  = SiS163.ndi1, PCI\VEN_1039&DEV_0163
