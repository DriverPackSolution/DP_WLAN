 PRISMA02.sys           = 1
; PRISMIOC.vxd           = 1,win9x
; PRISMNDI.dll           = 1,win9x
; PRISMAPI.dll           = 1
; PRISMSVR.exe           = 1

[SourceDisksFiles.ME]   ; WinMe
 PRISMA02.sys           = 1
; PRISMIOC.vxd           = 1,win9x
; PRISMNDI.dll           = 1,win9x
; PRISMAPI.dll           = 1
; PRISMSVR.exe           = 1

[SourceDisksFiles.X86]  ; Win2k/Xp
 PRISMA02.sys           = 1
; PRISMNDI.dll           = 1,win2k
; PRISMAPI.dll           = 1
; PRISMSVR.exe           = 1

