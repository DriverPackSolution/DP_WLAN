[Version]
 DriverVer = 09/01/2003, 3.1.3
 Signature           = "$Chicago$"
 Compatible          = 1
 Class               = Net
 ClassGUID           = {4d36e972-e325-11ce-bfc1-08002be10318}
 Provider            = %VER_VENDOR_STR%

[ControlFlags]
;Exclude all PNP adapters from user selection
 ExcludeFromSelect   = *

[Manufacturer]
 %VER_VENDOR_NAME_STR% = DeviceList

[DeviceList]
 %USB_DESC_STR%     = WLAN_USB1,        USB\VID_0CDE&PID_0005			       
 %USB_DESC_STR%     = WLAN_USB2,        USB\VID_0CDE&PID_0004			       
 %USB_DESC_STR%     = WLAN_USB3,        USB\VID_0CDE&PID_0003			       
 %USB_DESC_STR%	    = WLAN_USB4,    	USB\VID_09AA&PID_3642			       
 %USB_DESC_STR%     = WLAN_USB5,        USB\VID_9016&PID_182D			       
 
;==========================================
[WLAN_USB1.NT]    ; Win2k
 AddReg         = WLAN_USB1.reg, COMMON_USB.reg, COMMON_NDIS.reg.NT, COMMON.reg
 CopyFiles      = WLAN_DRIVER.copy.NT
 BusType        = 15     ; USB
 Characteristics= 0x84  ; NCF_PHYSICAL | NCF_HAS_UI

[WLAN_USB1.NT.Services]
 AddService     = "WlanUIB", 2, WLAN_DRIVER_USB.Service, WLAN_DRIVER.EventLog

[WLAN_USB1.reg]
 HKR,Ndi        ,DeviceID,0,"USB\Vid_0cde&Pid_0005"
 HKR,           ,PlatformId,0,"0"
 HKR,           ,VendorDesc,0,%USB_DESC_STR%

;==========================================
[WLAN_USB2.NT]    ; Win2k
 AddReg         = WLAN_USB2.reg, COMMON_USB.reg, COMMON_NDIS.reg.NT, COMMON.reg
 CopyFiles      = WLAN_DRIVER.copy.NT
 BusType        = 15     ; USB
 Characteristics= 0x84  ; NCF_PHYSICAL | NCF_HAS_UI

[WLAN_USB2.NT.Services]
 AddService     = "WlanUIB", 2, WLAN_DRIVER_USB.Service, WLAN_DRIVER.EventLog

[WLAN_USB2.reg]
 HKR,Ndi        ,DeviceID,0,"USB\VID_0CDE&PID_0004"
 HKR,           ,PlatformId,0,"0"
 HKR,           ,VendorDesc,0,%USB_DESC_STR%

;==========================================
[WLAN_USB3.NT]    ; Win2k
 AddReg         = WLAN_USB3.reg, COMMON_USB.reg, COMMON_NDIS.reg.NT, COMMON.reg
 CopyFiles      = WLAN_DRIVER.copy.NT
 BusType        = 15     ; USB
 Characteristics= 0x84  ; NCF_PHYSICAL | NCF_HAS_UI

[WLAN_USB3.NT.Services]
 AddService     = "WlanUIB", 2, WLAN_DRIVER_USB.Service, WLAN_DRIVER.EventLog

[WLAN_USB3.reg]
 HKR,Ndi        ,DeviceID,0,"USB\VID_0CDE&PID_0003"
 HKR,           ,PlatformId,0,"0"
 HKR,           ,VendorDesc,0,%USB_DESC_STR%
 
;==========================================
[WLAN_USB4.NT]    ; Win2k
 AddReg         = WLAN_USB4.reg, COMMON_USB.reg, COMMON_NDIS.reg.NT, COMMON.reg
 CopyFiles      = WLAN_DRIVER.copy.NT
 BusType        = 15     ; USB
 Characteristics= 0x84  ; NCF_PHYSICAL | NCF_HAS_UI

[WLAN_USB4.NT.Services]
 AddService     = "WlanUIB", 2, WLAN_DRIVER_USB.Service, WLAN_DRIVER.EventLog

[WLAN_USB4.reg]
 HKR,Ndi        ,DeviceID,0,"USB\VID_09AA&PID_3642"
 HKR,           ,PlatformId,0,"0"
 HKR,           ,VendorDesc,0,%USB_DESC_STR%
  
;==========================================
[WLAN_USB5.NT]    ; Win2k
 AddReg         = WLAN_USB5.reg, COMMON_USB.reg, COMMON_NDIS.reg.NT, COMMON.reg
 CopyFiles      = WLAN_DRIVER.copy.NT
 BusType        = 15     ; USB
 Characteristics= 0x84  ; NCF_PHYSICAL | NCF_HAS_UI

[WLAN_USB5.NT.Services]
 AddService     = "WlanUIB", 2, WLAN_DRIVER_USB.Service, WLAN_DRIVER.EventLog

[WLAN_USB5.reg]
 HKR,Ndi        ,DeviceID,0,"USB\VID_9016&PID_182D"
 HKR,           ,PlatformId,0,"0"
 HKR,           ,VendorDesc,0,%USB_DESC_STR%

;###############################################################################

[WLAN_DRIVER_USB.Service]
 DisplayName    = %WLAN_SERVICE_DISPLAY%
 ServiceType    = 1    ; SERVICE_KERNEL_DRIVER
 StartType      = 3    ; SERVICE_DEMAND_START
 ErrorControl   = 1    ; NORMAL
 ServiceBinary  = %12%\WlanUIB.sys
 LoadOrderGroup = NDIS

[WLAN_DRIVER.EventLog]
 AddReg         = WLAN_DRIVER.EventLog.reg

[WLAN_DRIVER.EventLog.reg]
 HKR,           ,EventMessageFile       ,0x00020000     ,"%%SystemRoot%%\System32\netevent.dll"
 HKR,           ,TypesSupported         ,0x00010001     ,7

;###############################################################################

[COMMON_USB.reg]
 HKR,                   ,WLANIOC		,0      ,"XI-735"
 HKR,NDI                ,Service                ,0      ,"WlanUIB"
 HKR,NDI                ,CardType               ,0      ,"USB"
 HKR,                   ,BusType                ,0      ,"15"
 HKR,                   ,DeviceVxDs             ,0      ,"WlanUIB.sys"

[COMMON_NDIS.reg.NT]
 HKR,NDI\Interfaces     ,UpperRange             ,0      ,"ndis5"
 HKR,NDI\Interfaces     ,LowerRange             ,0      ,"ethernet"

[COMMON.reg]

;Power save mode
