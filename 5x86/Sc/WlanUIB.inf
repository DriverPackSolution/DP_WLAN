 HKR, NDI\params\PSMode\enum,1,,"Disable"
 HKR, NDI\params\PSMode\enum,2,,"Always Enable"
 HKR, NDI\params\PSMode\enum,3,,"Auto Enable"
 HKR, NDI\params\PSMode,optional,0,1

