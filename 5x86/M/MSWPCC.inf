 FRAGTHRESH_STR         = "Fragmentation Threshold"
 POWER_SAVE_STR         = "Power Save Mode"
 DSCHANNEL_STR          = "Channel"
 NETWORKTYPE_STR        = "Network Type"
 NETWORKTYPE_P0_STR     = "Peer to Peer"
 NETWORKTYPE_P1_STR     = "Access Point"
