	DelReg		= common.delreg
	CopyFiles	= MSFT43XX.sys.files
DriverVer=07/07/2003, 3.20.26.0

[MSFT43XG3]
	AddReg		= MSFT43XX.win.reg, MSFT43XX.win.msft.reg, common.reg, g.options.reg, mn730.reg
	DelReg		= common.delreg
	CopyFiles	= MSFT43XX.sys.files
DriverVer=07/07/2003, 3.20.26.0

[MSFT43XX.win.reg]
	HKR,	Ndi,	DeviceID,		,	"PCI\VEN_14E4&DEV_4320"
	HKR,	,		EnumPropPages,		0,	"netdi.dll,EnumPropPages"
	HKR,	NDIS,	MajorNdisVersion,	1,	03
	HKR,	NDIS,	MinorNdisVersion,	1,	0A
	HKR,	NDIS,	LogDriverName,		,	"MSFT43XX"
