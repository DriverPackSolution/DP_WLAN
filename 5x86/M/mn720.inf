	Provider	= %Msft%
	Compatible	= 1
DriverVer=07/07/2003, 3.20.26.0
	CatalogFile=MN720.CAT

[Manufacturer]
	%Msft% = Microsoft

[ControlFlags] 
	ExcludeFromSelect = *

[Microsoft]
	%MSFT720G.DeviceDesc% = MSFT43XG2, PCI\VEN_14E4&DEV_4325&SUBSYS_00031414
	%MSFT730G.DeviceDesc% = MSFT43XG3, PCI\VEN_14E4&DEV_4325&SUBSYS_00041414

[MSFT43XG2]
