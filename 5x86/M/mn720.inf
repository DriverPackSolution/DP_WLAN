	HKR,	NDIS,	MinorDriverVersion,	1,	0
	HKR,	Ndi\Install,	,		,	"MSFT43XX.ndi.install"
	HKR,	Ndi\Interfaces,	DefUpper,	0,	"ndis3"
	HKR,	Ndi\Interfaces,	DefLower,	0,	"ethernet"
	HKR,	Ndi\Interfaces,	UpperRange,	0,	"ndis3"
	HKR,	Ndi\Interfaces,	LowerRange,	0,	"ethernet"

[MSFT43XX.win.msft.reg]
	HKR,	Ndi,	HelpText,		,	%MSFT43XX_HELP%

[MSFT43XX.ndi.install]
	AddReg		= MSFT43XX.ndi.reg

[MSFT43XX.ndi.reg]
	HKR,	,	DevLoader,		0,	"*ndis"
	HKR,	,	DeviceVxDs,		0,	"mn720-50.sys"

[MSFT43XX.sys.files]
	mn720-50.sys,mn720-50.sys,,2

[MSFT43XG2.NT]
	Characteristics	= 0x84
	BusType		= 5
	AddReg		= MSFT43XX.reg, MSFT43XX.msft.reg, common.reg, g.options.reg
	DelReg		= common.delreg
	CopyFiles	= MSFT43XX.files.NT
DriverVer=07/07/2003, 3.20.26.0

[MSFT43XG2.NT.Services]
	AddService = MSFT43XX, 2, MSFT43XX.Service, common.EventLog

[MSFT43XG3.NT]
	Characteristics	= 0x84
	BusType		= 5
	AddReg		= MSFT43XX.reg, MSFT43XX.msft.reg, common.reg, g.options.reg, mn730.reg
	DelReg		= common.delreg
	CopyFiles	= MSFT43XX.files.NT
DriverVer=07/07/2003, 3.20.26.0

[MSFT43XG3.NT.Services]
	AddService = MSFT43XX, 2, MSFT43XX.Service, common.EventLog

[MSFT43XX.reg]
	HKR,	Ndi\Interfaces,	UpperRange,	,	"ndis5"
	HKR,	Ndi\Interfaces,	LowerRange,	,	"ethernet"

[MSFT43XX.msft.reg]
	HKR,	Ndi,	HelpText,		,	%MSFT43XX_HELP%
	HKR,	Ndi,	Service,		0,	"MSFT43XX"

[MSFT43XX.Service]
	DisplayName	= %MSFT43XX.Service.DispName%
	ServiceType	= 1
	StartType	= 3
	ErrorControl	= 1
	ServiceBinary	= %12%\mn720-50.sys
	LoadOrderGroup	= NDIS

[common.EventLog]
	AddReg = common.AddEventLog.reg

[common.AddEventLog.reg]
	HKR,	,	EventMessageFile,	0x00020000,	"%%SystemRoot%%\System32\netevent.dll"
	HKR,	,	TypesSupported,		0x00010001,	7

