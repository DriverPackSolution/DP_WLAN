%SWIDevice77501%  = QportInstall01, USB\VID_114F&PID_68A2&MI_00

[QportInstall00.NT]
CopyFiles = gobiUsbDriverFiles
AddReg    = All00, QCAddReg, DevFunction2

[QportInstall01.NT]
CopyFiles = gobiUsbDriverFiles
AddReg    = All00, QCAddReg


[gobiUsbDriverFiles]
swg3kser01.sys

[DestinationDirs]
DefaultDestDir = 12

[QportInstall00.NT.Services]
AddService = swg3kser01,  0x00000002, gobi_USBSerialService

[QportInstall01.NT.Services]
AddService = swg3kser01,  0x00000002, gobi_USBSerialService

[gobi_USBSerialService]
DisplayName   = %GOBI3KSER%
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %12%\swg3kser01.sys

[All00]
HKR,,NTMPDriver,,swg3kser01.sys

[DevFunction2]
HKR,,QCDeviceFunction,0x00010001,2

[QCAddReg]
HKR,,PortSubClass,1,01
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[Strings]
