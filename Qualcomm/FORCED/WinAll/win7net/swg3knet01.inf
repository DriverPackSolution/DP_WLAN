gobiwwan.Sierra7700           = "Sierra Wireless MC7700 - Gobi(TM) 4000 HS-USB Mobile Broadband Device 68A2"
gobiwwan.Sierra7750           = "Sierra Wireless MC7750 - Gobi(TM) 4000 HS-USB Mobile Broadband Device 68A2"

