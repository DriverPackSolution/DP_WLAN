%QcomDevice2E1D1%  = QportInstall00, USB\VID_03F0&PID_2E1D&MI_01

[QportInstall00.NT]
CopyFiles=QCUsbDriverFiles
AddReg = All00, AddReg

[QCUsbDriverFiles]
qcusbserhp.sys

[DestinationDirs]
DefaultDestDir = 12

[QportInstall00.NT.Services]
AddService=qcusbserhp,  0x00000002, QC_USBSerialService

[QC_USBSerialService]
DisplayName   = %QCUSBSER%
ServiceType   = 1
StartType     = 3
ErrorControl  = 1
ServiceBinary = %12%\qcusbserhp.sys

[All00]
HKR,,NTMPDriver,,qcusbserhp.sys

[AddReg]
HKR,,PortSubClass,1,01
HKR,,EnumPropPages32,,"MsPorts.dll,SerialPortPropPageProvider"

[Strings]
QCUSBSER = "HP USB Device for Legacy Serial Communication"
QcomSrcDisk = "HP USB Driver Disk"
QUALCOMM   = "Qualcomm Incorporated"
