[Version]
signature  = "$WINDOWS NT$"
Class      = Ports
Provider   = %QUALCOMM%
ClassGuid  = {4D36E978-E325-11CE-BFC1-08002BE10318}
DriverVer=01/12/2010,6.0.7.1
Catalogfile= qcusbserhp.cat

[ControlFlags]
