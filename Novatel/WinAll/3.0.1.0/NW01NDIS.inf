;-------------------------------------------------------------------------------
; INF Version Section
;-------------------------------------------------------------------------------
[version]
signature   = "$WINDOWS NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    =%Mfg%
Compatible  = 1
DriverVer   = 03/01/2007,1.0.3.6
CatalogFile.NT = NWADI01.cat
CatalogFile.NTAMD64 = NWADI164.cat

;-------------------------------------------------------------------------------
; INF Manufactuerer Section
;-------------------------------------------------------------------------------
[Manufacturer]
%Mfg%=Models,NTamd64


;-------------------------------------------------------------------------------
; INF ControlFlags Section
;-------------------------------------------------------------------------------
[ControlFlags]
; Note: Uncomment next line if your NIC is not to be installed manually
ExcludeFromSelect = *

;-------------------------------------------------------------------------------
; INF Models Section
;-------------------------------------------------------------------------------
[Models]
%NW000%=*novatel.ndi, NWADI\DELL-VNIC_413C_8114
