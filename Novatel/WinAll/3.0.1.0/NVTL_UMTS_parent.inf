LogConfig = nwDevice.NT.LogConfigOverride13
LogConfig = nwDevice.NT.LogConfigOverride14
LogConfig = nwDevice.NT.LogConfigOverride15


;-------------------------------------------------------------------------------
; INF Strings Section
;-------------------------------------------------------------------------------
[Strings]
nwClass        = "MultiFunction"
nwProvider     = "Novatel Wireless Inc"
nwCatalogFile  = "NwtUMTS"
nwDevice0      = "Novatel Wireless UMTS Modem Parent"


