signature  = "$WINDOWS NT$"
Class      = Ports
Provider   = %NW%
ClassGuid  = {4D36E978-E325-11CE-BFC1-08002BE10318}
DriverVer  = 02/28/2007,3.0.0.4
CatalogFile.NT = NWDelMdm.cat
CatalogFile.NTAMD64 = nwDelx64.cat

;-------------------------------------------------------------------------------
; INF Manufactuerer Section
;-------------------------------------------------------------------------------
